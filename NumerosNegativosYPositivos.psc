Algoritmo NumerosNegativosYPositivos
	
	Definir numero1, numero2 Como Entero;
	Imprimir Sin Saltar "Digite el primer número ";
	Leer numero1;
	Imprimir Sin Saltar "Digite el segundo número ";
	Leer numero2;
	
	Si numero1 > 0 Entonces
		
		Imprimir "El primer número ", numero1 ," es positivo. ";
	FinSi
	
		Si numero2 > 0 Entonces
			
			Imprimir "El segundo número ", numero2 ," es positivo. ";	
		
	FinSi
	
FinAlgoritmo
